<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/merp4/unraid-docker-templates/master/templates/DockerExample.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:System</Category>
  <Name>DockerExample</Name>
  <Description>
    A basic .NET Core Blazor Docker container example.
  </Description>
  <Overview>
    A basic .NET Core Blazor Docker container example.
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/r/merrrp/dockerexample/</Registry>
  <GitHub></GitHub>
  <Repository>merrrp/dockerexample</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>4444</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>4445</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>WEBUI_PORT</Name>
      <Value>4444</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/appdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/userdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:32400]/web/index.html</WebUI>
  <Icon>https://raw.githubusercontent.com/merp4/unraid-docker-templates/master/templates/DockerExample.png</Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>